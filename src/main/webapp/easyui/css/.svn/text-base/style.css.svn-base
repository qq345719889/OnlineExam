*{
    font-size:12px;
}

.TopLink{color:#0076c9; position:absolute; right:0px; top:0px; background:url(../images/mask_white.png); height:20px; line-height:20px; padding:0px 10px;}
.TopLink A{ color:#0076c9; display:inline-block; margin:0px 5px;}
.TopLink A:hover{ color:#000;}
.TopLink A b{ display:inline-block; width:16px; height:16px; vertical-align:middle; background-image:url(../images/icon_topLink.png); background-repeat:no-repeat; margin-right:5px;}
.TopLink .icon1{ background-position:0px 0px;}
.TopLink .icon2{ background-position:0px -16px;}

.FormType{ }
.FormType table{ width:100%;}
.FormType th{white-space:nowrap; padding:3px 3px; text-align:right; font-size:12px; color:#5283a9; vertical-align:top; min-height:26px; line-height:26px; font-weight:normal; }
